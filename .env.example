CELERY_BROKER_URL=postgresql+psycopg://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_SERVER}:${POSTGRES_PORT}/${POSTGRES_DB}?schema=pgmq
CELERY_RESULT_BACKEND=postgresql+psycopg://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_SERVER}:${POSTGRES_PORT}/${POSTGRES_DB}

# Variables d'embedding
EMBEDDING_MODEL_ENDPOINT=https://api.openai.com/v1/embeddings
